{
  "name": "@gentrace/cli",
  "version": "1.0.0",
  "type": "module",
  "description": "Gentrace CLI",
  "repository": {
    "type": "git",
    "url": "git@github.com:gentrace/gentrace-node.git"
  },
  "keywords": [
    "gentrace",
    "cli"
  ],
  "author": "Gentrace",
  "license": "MIT",
  "bin": {
    "gentrace": "./src/index.js"
  },
  "scripts": {
    "gentrace": "gentrace",
    "build": "run-s clean typescript:compile rollup:compile",
    "typescript:compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "rollup:compile": "rollup --config rollup.config.mjs",
    "test": "jest"
  },
  "dependencies": {
    "@gentrace/core": "workspace:^2.1.15",
    "clipboardy": "^3.0.0",
    "dotenv-cli": "^7.3.0",
    "import-jsx": "^5.0.0",
    "ink": "^4.4.1",
    "ink-text-input": "^5.0.1",
    "ora": "^7.0.1",
    "react": "^18.2.0",
    "yargs": "^17.7.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@gentrace/tsconfig": "workspace:*",
    "@mswjs/interceptors": "^0.17.9",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^12.11.5",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "msw": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.5",
    "tslib": "^2.6.2",
    "typescript": "~5.1.3"
  }
}

{
  "name": "@gentrace/node",
  "version": "0.10.0",
  "description": "Node.js library for the Gentrace API",
  "repository": {
    "type": "git",
    "url": "git@github.com:gentrace/gentrace-node.git"
  },
  "keywords": [
    "gentrace",
    "openapi-client"
  ],
  "author": "Gentrace",
  "license": "MIT",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./openai": {
      "import": "./openai.mjs",
      "require": "./openai.js",
      "default": "./openai.js",
      "types": "./openai.d.ts"
    },
    "./pinecone": {
      "import": "./pinecone.mjs",
      "require": "./pinecone.js",
      "default": "./pinecone.js",
      "types": "./pinecone.d.ts"
    }
  },
  "scripts": {
    "build": "run-s clean typescript:compile rollup:compile copy-package",
    "clean": "rimraf dist",
    "typescript:compile": "tsc -p tsconfig.json",
    "rollup:compile": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "copy-package": "cp package.json dist/package.json",
    "test": "jest"
  },
  "peerDependencies": {
    "@pinecone-database/pinecone": "> 0.0.1 < 1.0.0",
    "openai": "> 3.0.0 < 4.0.0"
  },
  "peerDependenciesMeta": {
    "@pinecone-database/pinecone": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "mustache": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "^0.0.12",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.1",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/node": "^12.11.5",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "json-stable-stringify": "^1.0.2",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "openai": "^3.2.1",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}

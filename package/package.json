{
  "name": "@gentrace/node",
  "version": "0.7.19",
  "description": "Node.js library for the Gentrace API",
  "repository": {
    "type": "git",
    "url": "git@github.com:gentrace/gentrace-node.git"
  },
  "keywords": [
    "gentrace",
    "openapi-client"
  ],
  "author": "Gentrace",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./openai": {
      "types": "./dist/providers/getters/openai.d.ts",
      "default": "./dist/providers/getters/openai.js"
    },
    "./pinecone": {
      "types": "./dist/providers/getters/pinecone.d.ts",
      "default": "./dist/providers/getters/pinecone.js"
    }
  },
  "scripts": {
    "build": "run-s clean compile copy-providers",
    "clean": "rimraf dist && rimraf openai/dist && rimraf pinecone/dist",
    "copy-providers": "cp -r dist/openai openai/dist && cp -r dist/pinecone pinecone/dist",
    "compile": "tsc -p tsconfig.json",
    "test": "run-s build jest",
    "jest": "jest"
  },
  "peerDependencies": {
    "@pinecone-database/pinecone": "> 0.0.1 < 1.0.0",
    "openai": "> 3.0.0 < 4.0.0"
  },
  "peerDependenciesMeta": {
    "@pinecone-database/pinecone": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "mustache": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "^0.0.12",
    "@types/jest": "^29.5.1",
    "@types/mustache": "^4.2.2",
    "@types/node": "^12.11.5",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "openai": "^3.2.1",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}

{
  "name": "@gentrace/node",
  "version": "0.7.7",
  "description": "Node.js library for the Gentrace API",
  "repository": {
    "type": "git",
    "url": "git@github.com:gentrace/gentrace-node.git"
  },
  "keywords": [
    "gentrace",
    "openapi-client"
  ],
  "author": "Gentrace",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./openai": {
      "types": "./dist/providers/getters/openai.d.ts",
      "default": "./dist/providers/getters/openai.js"
    },
    "./pinecone": {
      "types": "./dist/providers/getters/pinecone.d.ts",
      "default": "./dist/providers/getters/pinecone.js"
    }
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf dist",
    "compile": "tsc -p tsconfig.json"
  },
  "peerDependencies": {
    "@pinecone-database/pinecone": "> 0.0.1 < 1.0.0",
    "openai": "> 3.0.0 < 4.0.0"
  },
  "peerDependenciesMeta": {
    "@pinecone-database/pinecone": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "dependencies": {
    "axios": "^0.27.2",
    "mustache": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "^0.0.12",
    "@types/mustache": "^4.2.2",
    "@types/node": "^12.11.5",
    "@types/uuid": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "openai": "^3.2.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  }
}
